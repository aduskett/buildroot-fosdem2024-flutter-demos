image boot.vfat {
  vfat {
    files = {
      "kernel_2712.img",
      "bcm2712-rpi-5-b.dtb",
      "rpi-firmware/cmdline.txt",
      "rpi-firmware/config.txt",
      "#OVERLAY_DIR#",
    }
  }
  size = 32M
}

image sdcard.img {
  hdimage {}

  partition boot {
    partition-type = 0xC
    bootable = "true"
    image = "boot.vfat"
  }

  partition rootfs {
    partition-type = 0x83
    image = "rootfs.ext4"
  }
}
